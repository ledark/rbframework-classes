{
    "name": "ledark/rbframeworks",
    "type": "library",
    "description": "Utilitarios para RBFrameworks",
    "keywords": [
        "rbframeworks"
    ],
    "homepage": "https://github.com/ledark/rbframework-classes",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Bermejo",
            "email": "mestreledark@gmail.com",
            "homepage": "http://bermejo.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": "^6.5",
        "sergeytsalkov/meekrodb": "^2.2",
        "guzzlehttp/psr7": "^1.6",
        "eftec/bladeone": "^3.44",
        "eftec/bladeonehtml": "^1.4",
        "bramus/router": "^1.6",
        "symfony/cache": "^5.4",
        "phpmailer/phpmailer": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "RBFrameworks\\": "src/"
        },
        "files": [
            "_include.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0"
    },
    "archive": {
        "exclude": ["/tests", "/collections", "/*.test", "/functions"]
    }    
}
